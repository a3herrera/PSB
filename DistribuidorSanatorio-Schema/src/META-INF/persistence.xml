<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="DistribuidorSanatorio-Schema"
		transaction-type="RESOURCE_LOCAL">
		<non-jta-data-source>jdbc/sanatorio</non-jta-data-source>
		<class>app.schema.persons.Person</class>
		<class>app.schema.persons.PersonInformation</class>
		<class>app.security.Profile</class>
		<class>app.security.User</class>
		<class>app.security.Menu</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="eclipselink.query-results-cache" value="false" />
			<!-- <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/sanatorio" 
				/> -->
			<!-- <property name="javax.persistence.jdbc.user" value="sanatorio" /> -->
			<!-- <property name="javax.persistence.jdbc.password" value="sanatorio" 
				/> -->
			<!-- <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" 
				/> -->
		</properties>

	</persistence-unit>
</persistence>
